class Solution {
    public int fib(int n) {
        if(n < 2) return n;
        int n1 = 0;
        int n2 = 1;
        int temp = 0;

        for(int i =1 ; i < n ; i++){
            temp = n1;
            n1 = n2;
            n2 += temp;
            
        }
        return n2;
        
    }
}
